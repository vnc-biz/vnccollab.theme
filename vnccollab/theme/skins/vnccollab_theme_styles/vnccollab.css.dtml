#vnccollab-header.opened .headerTimeViewletShort {
  display: none;
  }
.headerTimeViewlet {
  background:url(&dtml-portal_url;/clock.png) no-repeat 100% 0;
  color:#fff;
  float:right;
  font-size:1.06em;
  line-height:1.41em;
  margin:40px 0 0;
  padding:20px 80px 20px 20px;
  width:200px;
  }
.headerTimeViewlet span {
  display:inline-block;
  vertical-align:middle;
  }
.headerTimeViewlet .day {
  background-color:#fff;
  border-radius:15px;
  -moz-border-radius:15px;
  color:#082736;
  font-weight:bold;
  padding:4px 7px 3px 6px;
  }
.headerTimeViewletShort {
  background-color:#12425f;
  color:#fff;
  font-size:1.06em;
  line-height:1.41em;
  padding:2px 40px;
  text-align:right;
  }
.actionLink {
  background:url(&dtml-portal_url;/sprite.png) no-repeat;
  height:18px;
  overflow:hidden;
  position:absolute;
  right:11px;
  text-indent:-9999px;
  top:5px;
  width:18px;
  }
.openLink {
  background-position:0 -18px;
  text-indent:9999px;
  }
div.carousel {
/*  background-color:#12425f;*/
  margin:0;

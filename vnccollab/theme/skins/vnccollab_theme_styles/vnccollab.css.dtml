  height: auto;
  width: 48px;
  border-radius: 4px;
  }
#stream-quick-post {
  margin: 0 48px 0 110px;
  position: relative;
  }
#stream-quick-post:before {
  background: url(&dtml-portal_url;/sprite.png) no-repeat -320px 0;
  content: "";
  display: block;
  height: 18px;
  left: -9px;
  position: absolute;
  top: 8px;
  width: 11px;
  }
#stream-quick-post-message {
  border: 1px solid #c4c4c4;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  box-shadow: 0 1px 2px #e9e9e9 inset;
  height: 120px;
  max-width: 100%;
  padding: 5px 9px;
  width: 100%;
  }
#stream-quick-post-message:focus {
  background-color: #fff;
  }
.streamQuickPostSubjectsWrapper {
  margin: 15px 0 0;
  position: relative;
  }
.streamQuickPostSubjectsWrapper:after {
  background: url(&dtml-portal_url;/sprite.png) no-repeat -331px 0;
  content: "";
  display: block;
  height: 4px;
  position: absolute;
  right: 13px;
  top: 15px;
  width: 7px;
  }
#stream-quick-post-subject {
  border: 1px solid #c4c4c4;
  background-color: #fbfbfb;
  background-image: -moz-linear-gradient(#f7f7f7,#fbfbfb);
  background-image: -ms-linear-gradient(#f7f7f7,#fbfbfb);
  background-image: -o-linear-gradient(#f7f7f7,#fbfbfb);
  background-image: -webkit-linear-gradient(#f7f7f7,#fbfbfb);
  background-image: linear-gradient(#f7f7f7,#fbfbfb);
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  color: #012a43;
  font-size: 12px;
  outline: none;
  padding: 7px 25px 7px 9px;
  width: 100%;
  }
.streamQuickPostSubjects {
  background-color: #fff;
  border: 1px solid #c4c4c4;
  box-shadow: 0 2px 3px #cfcfcf;
  color: #012a43;
  display: none;
  font-size: 12px;
  margin-top: -1px;
  padding: 5px 0 3px;
  position: absolute;
  width: 90%;
  }
.streamQuickPostSubjectsWrapper:hover .streamQuickPostSubjects {
  display: block;
  }
.streamQuickPostSubjects li {
  border-top: 1px solid #e5e5e5;
  display: block;
  padding: 5px 12px;
  }
.streamQuickPostSubjects li:first-child {
  border-top: 0;
  }
#stream-quick-post-submit {
  background-color: #dee2e8;
  background-image: -moz-linear-gradient(#dee2e8,#cfd6dd);
  background-image: -ms-linear-gradient(#dee2e8,#cfd6dd);
  background-image: -o-linear-gradient(#dee2e8,#cfd6dd);
  background-image: -webkit-linear-gradient(#dee2e8,#cfd6dd);
  background-image: linear-gradient(#dee2e8,#cfd6dd);
  border: 1px solid #738ea2;
  border-radius: 5px;
  box-shadow: 0 1px #e6e9ee inset;
  color: #012a43;
  cursor: pointer;
  display: block;
  margin: 15px auto;
  outline: none;
  padding: 5px 0;
  width: 114px;
  }


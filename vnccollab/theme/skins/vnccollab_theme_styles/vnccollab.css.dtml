/* @group Tabs */

/* Color differences between the two tab types */
#content ul.formTabs {
  background-color: #eee;
  border: none;
}
#edit-bar {
  -moz-border-radius: 0;
  -webkit-border-radius: 0;
  background-color: #ced5db;
  border-radius: 0;
  border: none;
  font-size: 16px;
  margin-bottom: 2em;
  min-height: 30px;
  height: 30px;
  padding-top: 0;
}
.portaltype-document #edit-bar {
  -moz-border-radius: 0;
  -webkit-border-radius: 0;
  background-color: #e1e5e7;
  border-radius: 0;
  border: none;
  border-bottom: 1px dashed rgba(0,42,67,0.2) !important;
  font-size: 0.84em;
  margin-bottom: 2em;
  min-height: 2.5em;
  height: 2.5em;
  padding-top: 0;
}
#content-views {
  float: left;
  background: transparent;
}

#content-views a {
  color: #012a43;
}

/* Shared CSS between the two tab types */
#edit-bar,
#content ul.formTabs,
#content-core ul.formTabs {
  -moz-border-radius: 0;
  -webkit-border-radius: 0;
  border: none;
  border-radius: 0;
  border-radius: 0;
  display: block;
  margin-left: 0;
}

#content ul.formTabs,
#content-core ul.formTabs {
  border-bottom: 1px solid #8599A5;
}

#content li.formTab,
#content-core li.formTab {
  display: inline;
  margin-right:0;
}
#content-views a,
#content li.formTab a,
#content-core li.formTab a {
  /* this isn't supported by all browsers, so we keep it in a separate rule */
  display: inline-block;
  border: none;
  line-height: 30px;
  margin: 0 ;
  padding: 0 1.0em;
  text-decoration: none;
}
.portaltype-document #content-views a,
.portaltype-document #content li.formTab a,
.portaltype-document #content-core li.formTab a {
  /* this isn't supported by all browsers, so we keep it in a separate rule */
  display: inline-block;
  border: none;
  line-height: 2.6em;
  margin: 0 ;
  padding: 0 15px;
  text-decoration: none;
  border-right: 1px solid rgba(0,42,67,0.2) !important;
}
#content-views li.selected a,
#content li.formTab a.selected,
#content-core li.formTab a.selected {
  font-weight: normal;
}
#content-views li.selected a,
#content-views li a:hover,
#content li.formTab a.selected,
#content li.formTab a:hover,
#content-core li.formTab a.selected,
#content-core li.formTab a:hover {
  -moz-border-radius: 0;
  -webkit-border-radius: 0;
  background: #8599a5;
  border-radius: 0;
  color: White;
  height: 30px;
}

.portaltype-document #content-views li.selected a,
.portaltype-document #content-views li a:hover,
.portaltype-document #content li.formTab a.selected,
.portaltype-document #content li.formTab a:hover,
.portaltype-document #content-core li.formTab a.selected,
.portaltype-document #content-core li.formTab a:hover {
  -moz-border-radius: 0;
  -webkit-border-radius: 0;
  background: transparent;
  border-radius: 0;
  color: rgba(0,42,67,1.0) !important;
  font-weight: 600;
  height: 2.6em;
  line-height: 2.6em;
  letter-spacing: 2px;
  padding: 0 15px;
}

#content-views li.selected a:after {

  border: 10px solid transparent;
  border-top-color: #333;

  top: 100%;
  left: 50%;
  margin-left: -10px;
}

/* @end */


@media screen {

/*---------- General Style ----------*/
body {
  background: #e6e9ee;
  color:#000;
  font:100% "Myriad Pro", Arial, FreeSans, sans-serif;
  min-width: 1060px;
  }

#ploneimage,
#plonelink {
  min-width: 0;
  }
#portal-top {
  width: 100%;
  }
#portal-header {
  margin: 0;
  text-align: center;
  }
#portal-header > * {
  text-align: initial;
  }
#portal-columns {
  background-color: #f2f2f2;
  border-bottom: 1px solid #c1ccd5;
  padding: 30px 0 50px;
  position: relative;
  width: 100%;
  }
#portal-columns:after,
#portal-footer:after {
  clear: both;
  content: "";
  display: block;
  visibility: hidden;
  }
#portal-columns.width-3\3a 4.position-0:before,
#portal-columns.width-1\3a 2:before {
  background: url(&dtml-portal_url;/columns-separator.png) no-repeat;
  background-size: 13px 100%;
  bottom: 60px;
  content: "";
  display: block;
  position: absolute;
  right: 330px;
  top: 30px;
  width: 30px;
  z-index: 1;
  }
#visual-column-wrapper {
  float: left;
  margin-right: -360px;
  width: 100%;
  }
#portal-column-content {
  float: right;
  margin-left: -360px;
  width: 100%;
  }
#portal-content {
  margin: 0 390px 0 30px;
  }
.width-full #portal-content {
  margin: 0 30px;
  }
.width-1\3a 2 #portal-content {
  margin: 0 390px;
  }
.width-3\3a 4.position-1\3a 4 #portal-content {
  margin: 0 30px 0 390px;
  }
#portal-column-one {
  float: left;
  margin: 0 30px;
  width: 300px;
  }
#portal-column-two {
  float: right;
  margin: 10px 30px 0 0;
  width: 300px;
  }
#portal-footer {
  background: none;
  margin: 15px auto;
  padding: 0;
  width: 960px;
  }

/*----- Personal Tools -----*/
#portal-personaltools-loggedin {
  font-size:13px;
  line-height:18px;
  float: right;
  margin: 26px 26px 0 0;
  }
.actionMenuUserImgWrapper {
  border: 1px solid #738ea2;
  border-right: 0;
  float:left;
  border-top-left-radius: 4px;
  border-bottom-left-radius: 4px;
  }
/*----- User Image -----*/
#user-image img {
  height: 45px;
  vertical-align: top;
  width: 45px;
  display: block;
  margin: 0 auto;
  border-radius: 50%;
  }
#user-image:after {
  content: "";
  height: 45px;
  width: 45px;
  display: block;
  margin: 0 auto;
  border-radius: 50%;
  box-shadow: inset 2px 2px 5px 0px rgba(0, 0, 0, 0.33);
  top: -4px;
  left: 16px;
  position: absolute;
  }
.userPreferences {
  background:url(&dtml-portal_url;/arrow-down.png) no-repeat right center;
  float: left;
  margin-right: 20px;
  position: relative;
  width: 76px;
  }
.userPreferencesHeader {
  margin-top: -4px;
  }
.userPreferencesHeader a {
  display: block;
  overflow: hidden;
  white-space: nowrap;
  }
.userPreferencesBody {
  width: 217px;
  padding: 5px 0;
  margin-left: -117px;
  margin-top: -5px;
  opacity: 0;
  visibility: hidden;
  z-index: 10;
  position: absolute;

  font-family: Arial;
  font-size: 12px;
  font-style: normal;
  font-weight: normal;

  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  -o-border-radius: 5px;
  border-radius: 5px;

  -webkit-box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.2);
  -moz-box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.2);
  box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.2);

  -webkit-transition-property:opacity, margin-top, visibility, margin-left;
  -webkit-transition-duration:0.3s, 0.2s, 0.3s, 0.2s;
  -webkit-transition-timing-function: ease-in-out, ease-in-out, ease-in-out, ease-in-out;

  -moz-transition-property:opacity, margin-top, visibility, margin-left;
  -moz-transition-duration:0.3s, 0.2s, 0.3s, 0.2s;
  -moz-transition-timing-function: ease-in-out, ease-in-out, ease-in-out, ease-in-out;

  -o-transition-property:opacity, margin-top, visibility, margin-left;
  -o-transition-duration:0.3s, 0.2s, 0.3s, 0.2s;
  -o-transition-timing-function: ease-in-out, ease-in-out, ease-in-out, ease-in-out;

  transition-property:opacity, margin-top, visibility, margin-left;
  transition-duration:0.3s, 0.2s, 0.3s, 0.2s;
  transition-timing-function: ease-in-out, ease-in-out, ease-in-out, ease-in-out;

  transform: perspective(700px);
  -webkit-transform: perspective(700px);

  background: #ffffff; /* Old browsers */
  background: -moz-linear-gradient(top, #ffffff 0%, #ffffff 50%, #f2f2f2 50%, #f2f2f2 100%); /* FF3.6+ */
  background: -ms-linear-gradient(top, #ffffff 0%,#ffffff 50%,#f2f2f2 50%,#f2f2f2 100%); /* IE10+ */
  background: -o-linear-gradient(top, #ffffff 0%,#ffffff 50%,#f2f2f2 50%,#f2f2f2 100%); /* Opera 11.10+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(50%,#ffffff), color-stop(50%,#f2f2f2), color-stop(100%,#f2f2f2)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, #ffffff 0%,#ffffff 50%,#f2f2f2 50%,#f2f2f2 100%); /* Chrome10+,Safari5.1+ */
  background: linear-gradient(to bottom, #ffffff 0%,#ffffff 50%,#f2f2f2 50%,#f2f2f2 100%); /* W3C */
  border: 1px solid #748fa3;
  color: #000000;
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#f2f2f2',GradientType=0 ); /* IE6-9 */
  }

.userPreferences:hover > .userPreferencesBody {
  opacity: 1;
  text-decoration:none;
  visibility: visible;
  overflow: visible;
  margin-top: 10px;
  display: inline;
}
.userPreferencesBody:before {
  content: "";
  width: 11px;
  height: 11px;
  margin-left: 149px;
  margin-top: -12px;

  display: block;
  position: absolute;

  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -o-transform: rotate(-45deg);
  transform: rotate(-45deg);

  background-color: #fff;

  border-top: 1px solid #748fa3;
  border-right: 1px solid #748fa3;
}
.userPreferences:hover .userPreferencesBody {
  display:block;
  }
.userPreferencesBody a {
  display: block;
  padding: 5px 9px;
  opacity: 1.0;
  color: #012a43 !important;
  }
.userPreferencesBody a:before {
  background:url(&dtml-portal_url;/sprite.png) no-repeat -190px 0;
  content: "";
  display: inline-block;
  height: 16px;
  margin-right: 5px;
  vertical-align: text-top;
  width: 16px;
  opacity: 0.7;
  }
.userPreferencesBody a:hover {
  text-decoration:none;
  }
.userPreferencesBody h3 {
  padding-left: 16px;
  padding-top: 4px;
  padding-bottom: 8px;
  }
.userPreferencesBody ul {
  background-color: #f2f2f2;
  }
.userPreferencesBody ul li{
  border-top: 1px solid #D6D9DB !important;
  background-color: #F2F2F2;
  color: #012A43 !important;
  display: block;
  height: 34px;
  line-height: 26px;
  padding: 0 8px;
}

#personaltools-mystuff a:before {
  background-position: -190px -20px;
  }
#personaltools-dashboard a:before {
  background-position: -206px 0;
  }

/*----- Language Selector -----*/

#personaltools-languageselector {
  padding: 7px 16px 0px;
  cursor: pointer;
}
#personaltools-languageselector ul {
  opacity: 0;
  visibility: hidden;
  border: 1px solid #D6D9DB;
  display: block;
}
#personaltools-languageselector:hover > ul {
  opacity: 1;
  text-decoration:none;
  visibility: visible;
  overflow: visible;
  margin-top: -2px;
}

#portal-languageselector {
  background-color: #dee3e8;
  background-image: -moz-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -ms-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -o-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -webkit-linear-gradient(#dee3e8,#cfd6dd);
  background-image: linear-gradient(#dee3e8,#cfd6dd);
  border: 1px solid #738ea2;
  border-radius: 4px;
  box-shadow: 0 1px #e6e9ee inset;
  float: left;
  font-size: 14px;
  font-weight:normal;
  line-height: 18px;
  margin-right: 10px;
  padding: 28px 24px 4px 0;
  width: 92px;
  /*display: none;*/
  }
#vnc-languageselector {
  box-shadow: 0px 4px 5px rgba(0, 0, 0, 0.1);
}
#vnc-languageselector li {
  height: 30px;
  line-height: 23px;
  padding-left: 0;
}
#vnc-languageselector a {
  padding-left: 0;
}
#vnc-languageselector a:before {
  background: none;
}
#vnc-languageselector .currentLanguage a {
  font-weight: bold;
}
select#vnc-languageselector {
  width: 200px;
}
#portal-languageselector:after {
  background:url(&dtml-portal_url;/sprite.png) no-repeat -206px -20px;
  content: "";
  display: block;
  height: 8px;
  position: absolute;
  right: 7px;
  top: 12px;
  width: 13px;
  }
#portal-languageselector li {
  display:block;
  }
#portal-languageselector a {
  display: none;
  padding: 3px 9px;
  }
#portal-languageselector:hover a {
  display: block;
  overflow: hidden;
  white-space: nowrap;
  }

#selected-language {
  background-color: #ced5db;
  padding: 7px 30px 7px 13px;
  text-align: left;
  border-radius: 3px;
  display: block;
  height: 17px;
  line-height: 17px;
}
#selected-language:after {
  content: "";
  background:url(&dtml-portal_url;/arrow-down.png) no-repeat right center;
  float: left;
  margin-right: 20px;
  position: absolute;
  width: 76px;
  height: 16px;
  width: 12px;
  right: 10px;
}

/*----- Logout Link -----*/
.logoutLink {
  background: url(&dtml-portal_url;/logout.png) no-repeat center center;
  display: inline-block;
  height: 34px;
  overflow: hidden;
  text-indent: -9999px;
  width: 34px;
  margin: 0;
  }

/*----- Login -----*/
.loginFormWrapper {
  position: absolute;
  right: 0;
  top: 52px;
  }
.loginFormControls label {
  font-weight:normal;
  margin:0 0 3px;
  vertical-align:middle;
  }
.loginFormControls input::-webkit-input-placeholder {
  color: #738ea2;
  }
input.loginFormName,
input.loginFormEmail,
input.loginFormPassword {
  background-color: #dee3e8;
  background-image: -moz-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -ms-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -o-linear-gradient(#dee3e8,#cfd6dd);
  background-image: -webkit-linear-gradient(#dee3e8,#cfd6dd);
  background-image: linear-gradient(#dee3e8,#cfd6dd);
  border: 1px solid #738ea2;
  box-shadow: 0 1px #e6e9ee inset;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  color: #738ea2;
  display: inline-block;
  font-size: 14px;
  height: 32px;
  line-height: 17px;
  margin-right: 7px;
  padding: 6px 9px;
  vertical-align: top;
  width: 140px;
  }
button::-moz-focus-inner,
input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
  padding: 0;
  border: 0;
  }
.loginFormSubmit {
  background-color: #12425f;
  border: 1px solid #012a43;
  color:#fff;
  cursor: pointer;
  font-size: 14px;
  height: 32px;
  padding: 0 9px;
  text-align:center;
  vertical-align: top;
  }

/*----- Logo -----*/
#portal-logo {
  margin:15px 0 15px 30px;
  float: left;
  }

/*----- Searchbox -----*/
#portal-searchbox {
  background-color:#f5f5f5;
  border:1px solid #738ea2;
  border-radius: 5px;
  box-shadow: 1px 1px 2px #d9d9d9 inset;
  display: inline-block;
  font-size:1em;
  float:none;
  padding: 0;
  width: 268px;
  height: 44px;
  margin: 22px 0 0 0;
  position: relative;
  vertical-align: top;
  }
#portal-searchbox .searchField {
  background-color: transparent;
  border: 0;
  color: #738EA2;
  font-size: 17px;
  outline: none;
  padding: 10px;
  vertical-align: top;
  width: 215px;
  height: 24px;
  display: inline-block;
  }
input.searchField {
  -moz-appearance: none;
  }
#portal-searchbox .searchButton {
  background: url(&dtml-portal_url;/sprite.png) no-repeat -154px 10px;
  border: 0;
  color: #738EA2;
  font-size: 17px;
  outline: none;
  padding: 10px;
  vertical-align: top;
  width: 20px;
  display: inline-block;
  color: transparent;
  margin-top: 2px;
  }

#portal-searchbox .LSSpin {
  background: url(&dtml-portal_url;/search_spinner.gif) no-repeat -1px 0;
  position: absolute;
  content: " ";
  display: none;
  top: 11px;
  width: 22px;
  height: 22px;
  right: 35px;
  }
#portal-searchbox .LSSpin.on {
  display: block;
  }

#portal-searchbox  input[type="submit"]:hover {
  color: transparent !important;
}
.searchSection {
  display:none;
  }
#LSResult {
  background:none;
  margin:0;
  right: -127px;
  top:50px;
  z-index: 9999;
  }
#LSResult fieldset {
  border:0;
  margin:0;
  padding:0 0 1em;
  }
#livesearchLegend {
  background-color: #f5f5f5;
  border: 0;
  box-shadow: 3px 0 2px -2px #D9D9D9 inset;
  border-left: 1px solid #738ea2;
  border-right: 1px solid #738ea2;
  color: #f5f5f5;
  height: 15px;
  left: 130px;
  margin:0;
  overflow: hidden;
  padding: 0;
  position:absolute;
  top: -14px;
  width: 268px;
  }
.LSIEFix {
  background: #f5f5f5;
  border: 1px solid #738ea2;
  border-radius: 3px;
  box-shadow: 0 3px 8px rgba(1, 42, 67, 0.25);
  padding: 0;
  }
.LSRow {
  padding:0 17px;
  width: 500px;
  }
#LSNothingFound {
  color: #8997a4;
  padding:0;
  width: 500px;
  text-align: center;
  margin-top: 6px;
  }
.LSRow a:link {
  color: #012a43;
  vertical-align: middle;
  }
.icons-on .livesearchContainer .contenttype-news-item {
  background-position: 0 -1288px;
  }
.icons-on .livesearchContainer .contenttype-document {
  background-position: 0 7px;
  }
.icons-on .livesearchContainer .contenttype-folder {
  background-position: 0 -640px;
  }
.icons-on .livesearchContainer .contenttype-topic {
  background-position: 0 -1720px;
  }
.icons-on .livesearchContainer .contenttype-link {
  background-position: 0 -1072px;
  }
.icons-on .contenttype-event {
  background-position: 0 -208px;
  }
.LSRow a[href="search_form"] {
  background-color: #e7e9ec;
  border-top: 1px solid #d9dfe3;
  display: block;
  font-size: 12px;
  font-weight: bold !important;
  line-height: 16px;
  margin: 10px -17px 0;
  padding: 12px 17px 6px;
  }
.LSRow a[href="search_form"]:before {
  background: url(&dtml-portal_url;/sprite.png) no-repeat -173px 0;
  content: "";
  display: inline-block;
  height: 16px;
  margin-right: 10px;
  vertical-align: middle;
  width: 15px;
  }
.LSRow a[href^="search?SearchableText="] {
  background-color: #e7e9ec;
  display: block;
  font-size: 12px;
  font-weight: bold !important;
  line-height: 16px;
  margin: 0 -17px;
  padding: 0 17px 11px;
  }
.LSRow a[href^="search?SearchableText="]:before {
  background: url(&dtml-portal_url;/sprite.png) no-repeat -173px -20px;
  content: "";
  display: inline-block;
  height: 16px;
  margin-right: 10px;
  vertical-align: middle;
  width: 15px;
  }
.LSRow:last-child a {
  border-radius: 0 0 3px 3px;
  padding-bottom: 11px;
  }
#LSResult .LSDescr {
  color: #012a43;
  font-size: 12px;
  line-height: 16px;
  }

/*----- Global Navigation -----*/
#portal-globalnav {
  background: rgba(0, 42, 67, 0.1);
  background-size: 100% 25px;
  border-top: 1px solid #B0B8BC;
  border-bottom: 1px solid #B0B8BC;
  line-height: 20px;
  height: 42px;
  margin: 0;
  padding: 0;
  text-align: center;
  }
#portal-globalnav li a {
  background-color: transparent;
  border: 0;
  color: #012a43;
  font-size: 1.4em;
  font-weight: 300;
  font-family: Helvetica;
  min-width: 0;
  padding: 11px 15px;
  width: 75px
  }
#portal-globalnav li a:hover,
#portal-globalnav li.selected a,
#portal-globalnav li.selected:hover a {
  background-color:transparent;
  font-weight: bold;
  color: #012a43;
  }

#portal-globalnav li.selected a:after {
  background-color: #f2f2f2;
  content: "";
  width: 11px;
  height: 11px;
  margin-left: 33px;
  margin-top: 5px;

  display: block;
  position: absolute;

  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -o-transform: rotate(-45deg);
  transform: rotate(-45deg);

  border-right: 1px solid #B0B8BC;
  border-top: 1px solid #B0B8BC;
  }

/*----- VNC Collab Header -----*/
